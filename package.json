{
  "name": "lcgc-fe",
  "description": "理财工场-前端模板",
  "private": true,
  "scripts": {
    "clean": "gulp clean",
    "start": "yarn run dev",
    "s-dev":
      "yarn run server & export SOLAR_FE_NODEJS=true && gulp dev & webpack -w --progress --display-error-details",
    "dev": "gulp dev & webpack -w --progress --display-error-details",
    "server": "node_modules/nodemon/bin/nodemon.js server/bin/www",
    "release": "gulp clean && webpack  --config  webpack.prod.config.js && gulp release && gulp clean_dist",
    "dll": "webpack --config webpack.vender.config --progress",
    "precommit-msg": "echo 'Pre-commit checks start...' && exit 0",
    "lint": "eslint src --ext .js --ext .jsx --cache --ignore-pattern .gitignore",
    "create": "node bin/create.js"
  },
  "pre-commit": ["precommit-msg", "lint"],
